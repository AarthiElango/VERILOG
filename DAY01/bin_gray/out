#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5cbdbb3ec550 .scope module, "bin_graytb" "bin_graytb" 2 1;
 .timescale 0 0;
v0x5cbdbb401fe0_0 .var "b", 3 0;
v0x5cbdbb4020a0_0 .net "g", 3 0, v0x5cbdbb401ea0_0;  1 drivers
S_0x5cbdbb3ec6e0 .scope module, "uut" "bin_gray" 2 5, 3 1 0, S_0x5cbdbb3ec550;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "b";
    .port_info 1 /OUTPUT 4 "g";
v0x5cbdbb3ec8e0_0 .net "b", 3 0, v0x5cbdbb401fe0_0;  1 drivers
v0x5cbdbb401ea0_0 .var "g", 3 0;
E_0x5cbdbb3b2ee0 .event anyedge, v0x5cbdbb3ec8e0_0;
    .scope S_0x5cbdbb3ec6e0;
T_0 ;
    %wait E_0x5cbdbb3b2ee0;
    %load/vec4 v0x5cbdbb3ec8e0_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5cbdbb401ea0_0, 4, 1;
    %load/vec4 v0x5cbdbb3ec8e0_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x5cbdbb3ec8e0_0;
    %parti/s 1, 2, 3;
    %xor;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5cbdbb401ea0_0, 4, 1;
    %load/vec4 v0x5cbdbb3ec8e0_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x5cbdbb3ec8e0_0;
    %parti/s 1, 1, 2;
    %xor;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5cbdbb401ea0_0, 4, 1;
    %load/vec4 v0x5cbdbb3ec8e0_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0x5cbdbb3ec8e0_0;
    %parti/s 1, 0, 2;
    %xor;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5cbdbb401ea0_0, 4, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5cbdbb3ec550;
T_1 ;
    %vpi_call 2 11 "$monitor", "%0t | b = %04b | g = %04b", $time, v0x5cbdbb401fe0_0, v0x5cbdbb4020a0_0 {0 0 0};
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 13, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 14, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x5cbdbb401fe0_0, 0, 4;
    %delay 5, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "bin_graytb.v";
    "bin_gray.v";
