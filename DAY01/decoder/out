#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x60eb1cb730a0 .scope module, "decoder_2to4tb" "decoder_2to4tb" 2 1;
 .timescale 0 0;
v0x60eb1cb85e50_0 .var "d0", 0 0;
v0x60eb1cb85f20_0 .var "d1", 0 0;
v0x60eb1cb85ff0_0 .net "q", 3 0, v0x60eb1cb85d10_0;  1 drivers
S_0x60eb1cb73230 .scope module, "uut" "decoder_2to4" 2 5, 3 1 0, S_0x60eb1cb730a0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "d1";
    .port_info 1 /INPUT 1 "d0";
    .port_info 2 /OUTPUT 4 "q";
v0x60eb1cb73480_0 .net "d0", 0 0, v0x60eb1cb85e50_0;  1 drivers
v0x60eb1cb85c50_0 .net "d1", 0 0, v0x60eb1cb85f20_0;  1 drivers
v0x60eb1cb85d10_0 .var "q", 3 0;
E_0x60eb1cb39f10 .event anyedge, v0x60eb1cb85c50_0, v0x60eb1cb73480_0;
    .scope S_0x60eb1cb73230;
T_0 ;
    %wait E_0x60eb1cb39f10;
    %load/vec4 v0x60eb1cb85c50_0;
    %load/vec4 v0x60eb1cb73480_0;
    %concat/vec4; draw_concat_vec4
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %jmp T_0.4;
T_0.0 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x60eb1cb85d10_0, 0, 4;
    %jmp T_0.4;
T_0.1 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x60eb1cb85d10_0, 0, 4;
    %jmp T_0.4;
T_0.2 ;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x60eb1cb85d10_0, 0, 4;
    %jmp T_0.4;
T_0.3 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x60eb1cb85d10_0, 0, 4;
    %jmp T_0.4;
T_0.4 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x60eb1cb730a0;
T_1 ;
    %vpi_call 2 12 "$monitor", $time, " | d1 = %b, d0 = %b | q = %b", v0x60eb1cb85f20_0, v0x60eb1cb85e50_0, v0x60eb1cb85ff0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60eb1cb85f20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60eb1cb85e50_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60eb1cb85f20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60eb1cb85e50_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60eb1cb85f20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60eb1cb85e50_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60eb1cb85f20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60eb1cb85e50_0, 0, 1;
    %delay 10, 0;
    %delay 10, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "decoder_2to4tb.v";
