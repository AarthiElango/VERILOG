#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x60ff59fc6410 .scope module, "mux_4t01tb" "mux_4t01tb" 2 1;
 .timescale 0 0;
v0x60ff59fd92c0_0 .var "s0", 0 0;
v0x60ff59fd9390_0 .var "s1", 0 0;
v0x60ff59fd9460_0 .net "y", 1 0, v0x60ff59fd9180_0;  1 drivers
S_0x60ff59fc65a0 .scope module, "uut" "mux_4t01" 2 4, 3 1 0, S_0x60ff59fc6410;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "s1";
    .port_info 1 /INPUT 1 "s0";
    .port_info 2 /OUTPUT 2 "y";
v0x60ff59f8c490_0 .net "s0", 0 0, v0x60ff59fd92c0_0;  1 drivers
v0x60ff59fd90c0_0 .net "s1", 0 0, v0x60ff59fd9390_0;  1 drivers
v0x60ff59fd9180_0 .var "y", 1 0;
E_0x60ff59f8cb60 .event anyedge, v0x60ff59fd90c0_0, v0x60ff59f8c490_0;
    .scope S_0x60ff59fc65a0;
T_0 ;
    %wait E_0x60ff59f8cb60;
    %load/vec4 v0x60ff59fd90c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x60ff59f8c490_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x60ff59fd9180_0, 0, 2;
    %jmp T_0.3;
T_0.2 ;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x60ff59fd9180_0, 0, 2;
T_0.3 ;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x60ff59f8c490_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x60ff59fd9180_0, 0, 2;
    %jmp T_0.5;
T_0.4 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x60ff59fd9180_0, 0, 2;
T_0.5 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x60ff59fc6410;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60ff59fd9390_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60ff59fd92c0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60ff59fd9390_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60ff59fd92c0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60ff59fd9390_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60ff59fd92c0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60ff59fd9390_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60ff59fd92c0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x60ff59fc6410;
T_2 ;
    %vpi_call 2 17 "$monitor", $time, " | s1=%b | s0=%b | y=%b", v0x60ff59fd9390_0, v0x60ff59fd92c0_0, v0x60ff59fd9460_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
